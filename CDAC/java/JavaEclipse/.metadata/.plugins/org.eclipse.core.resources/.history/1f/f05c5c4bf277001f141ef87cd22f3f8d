package MyPackage;

public class string1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s2="PG DAC";
		String s1="Have A Nice Day";
		System.out.println("Lenth of string is ="+s1.length());
		System.out.println("6th position  is ="+s1.charAt(6));
		System.out.println("Upper Case of string is ="+s1.toUpperCase());
		System.out.println("Lowe Case of string is ="+s1.toLowerCase());
		System.out.println("Copy String is ="+s1.concat(s2));

	}

}

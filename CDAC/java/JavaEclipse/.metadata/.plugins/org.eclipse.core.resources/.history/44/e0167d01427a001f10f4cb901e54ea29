package Inheritance;

public class SingleInheritance {
	
         void show()	
         {
        	 System.out.println("It is a Super class");
        	 
        	 
        	 
         }
	
	

}
 class Derived1 extends SingleInheritance{
	 
	 public void show()
	 {
		 
		 
		 System.out.println("It is a Derived class");
	 }
	
	
}
